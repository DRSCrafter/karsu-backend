{
    "name": "expressts-backend",
    "version": "1.0.0",
    "description": "Express TypeScript backend with PostgreSQL and TypeORM",
    "main": "dist/server.js",
    "scripts": {
        "start": "ts-node-dev --respawn --transpile-only src/server.ts",
        "build": "tsc",
        "swagger:validate": "ts-node scripts/swagger-validate.js",
        "migration:generate": "typeorm-ts-node-commonjs -d src/database.ts migration:generate src/migrations/$npm_config_name",
        "migration:run": "typeorm-ts-node-commonjs -d src/database.ts migration:run"
    },
    "dependencies": {
        "bcryptjs": "^3.0.2",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "dotenv": "^17.2.1",
        "express": "^4.18.2",
        "express-rate-limit": "^8.0.1",
        "helmet": "^8.1.0",
        "jsonwebtoken": "^9.0.2",
        "pg": "^8.7.3",
        "reflect-metadata": "^0.1.13",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1",
        "typeorm": "^0.3.12",
        "zod": "^3.25.76"
    },
    "devDependencies": {
        "@types/cookie-parser": "^1.4.9",
        "@types/cors": "^2.8.19",
        "@types/express": "^4.17.13",
        "@types/express-rate-limit": "^6.0.2",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/swagger-jsdoc": "^6.0.4",
        "@types/swagger-ui-express": "^4.1.3",
        "swagger-cli": "^4.0.4",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.6.4"
    }
}
